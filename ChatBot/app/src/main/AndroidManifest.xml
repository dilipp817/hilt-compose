<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tracker.track">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name="com.demo.chatbot.app.ChatBotApplication"
        android:theme="@style/Theme.chatBot">
        <activity
            android:name="com.demo.chatbot.ui.containers.EntryPointActivity"
            android:exported="true"
            android:label="@string/title_activity_auth"
            android:theme="@style/Theme.chatBot" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.demo.chatbot.ui.containers.ChatActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.chatBot.NoActionBar">
        </activity>
    </application>
</manifest>